{
  "schemaVersion": 1,
  "id": "skyblockcore",
  "version": "@version@",

  "name": "SkyBlock Core",
  "description": "SkyBlock Core is a centralized mod for SkyBlock settings, menus, localization, events, and more.",
  "authors": [
    "Skyblock Mod Developers"
  ],
  "contributors": [
    "Ascynx",
    "WiViW",
    "Axle",
    "TGWaffles"
  ],
  "contact": {
    "sources": "https://github.com/SkyBlock-Central/SkyBlock-Core"
  },

  "license": "Apache-2.0",
  "icon": "assets/skyblockcore/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "io.github.skyblockcore.SkyBlockCore"
    ]
  },
  "mixins": [
    "skyblockcore.mixins.json"
  ],

  "custom": {
    "modmenu": {
      "badges": [ "library" ]
    },
    "update_checker": false
  },

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": "*",
    "minecraft": "~1.19.4",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
